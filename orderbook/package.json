{
  "name": "divvy-lib-orderbook",
  "version": "0.1.3",
  "license": "ISC",
  "description": "Convenient Orderbook class.",
  "files": [
    "dist/npm/*",
    "test/*"
  ],
  "scripts": {
    "clean": "rm -rf dist/npm && rm -rf build/flow",
    "typecheck": "babel --optional runtime --blacklist flow,es6.classes -d build/flow/ src/ && flow check",
    "compileflow": "babel --optional runtime --blacklist flow,es6.classes -d build/flow/ src/",
    "compile": "babel -D --optional runtime -d dist/npm/ src/",
    "prepublish": "npm run clean && npm run compile",
    "test": "istanbul test _mocha",
    "lint": "if ! [ -f eslintrc ]; then curl -o eslintrc 'https://raw.githubusercontent.com/xdv/javascript-style-guide/es6/eslintrc'; echo 'parser: babel-eslint' >> eslintrc; fi; eslint -c eslintrc Gulpfile.js **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xdv/divvy-lib-extensions.git"
  },
  "main": "dist/npm/",
  "keywords": [
    "divvy",
    "divvy-lib",
    "divvy-lib-orderbook"
  ],
  "engines": {
    "node": ">=0.12.0"
  },
  "dependencies": {
    "babel-runtime": "^5.8.29",
    "lodash": "^3.10.1",
    "divvy-address-codec": "^2.0.1",
    "divvy-binary-codec": "^0.1.2",
    "divvy-lib-value": "0.1.0"
  },
  "devDependencies": {
    "assert-diff": "^1.0.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.0.0",
    "eslint": "2.7.0",
    "flow-bin": "^0.191.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "mocha": "^10.1.0",
    "divvy-lib": "0.22.0",
    "webpack-stream": "^3.1.0"
  }
}
